C4_SRC = C4/C4.java C4/C4Helper.java
C5_SRC = C5/C5.java C5/C5Helper.java

all: s0 s1 s2 s3 s4 s5 s6 s7 c1 c2 c3 c4 c5

s0:
	$(JAVAC) $(JAVAFLAGS) $(S0_SRC)

s1:
	$(JAVAC) $(JAVAFLAGS) $(S1_SRC)
s2:
	$(JAVAC) $(JAVAFLAGS) $(S2_SRC)
s3:
	$(JAVAC) $(JAVAFLAGS) $(S3_SRC)
s4:
	$(JAVAC) $(JAVAFLAGS) $(S4_SRC)
s5:
	$(JAVAC) $(JAVAFLAGS) $(S5_SRC)
s6:
	$(JAVAC) $(JAVAFLAGS) $(S6_SRC)
s7:
	$(JAVAC) $(JAVAFLAGS) $(S7_SRC)
c1:
	$(JAVAC) $(JAVAFLAGS) $(C1_SRC)
c2:
	$(JAVAC) $(JAVAFLAGS) $(C2_SRC)
c3:
	$(JAVAC) $(JAVAFLAGS) $(C3_SRC)
c4:
	$(JAVAC) $(JAVAFLAGS) $(C4_SRC)
c5:
	$(JAVAC) $(JAVAFLAGS) $(C5_SRC)

clean:
	rm -f S0/*.class S1/*.class S2/*.class S3/*.class S4/*.class S5/*.class S6/*.class S7/*.class C1/*.class C2/*.class C3/*.class C4/*.class C5/*.class